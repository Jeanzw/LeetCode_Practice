class Solution:
    def longestPalindrome(self, s: str) -> str:
        if len(s) == 0:
            return ""
        res = s[0]
        for i in range(len(s)):
            for j in range(i+1,len(s)):
                #print(s[i],s[j])
                if s[i] == s[j]:
                    a = s[i:j+1]
                    if a[::-1] == a:
                        if len(res) < len(a):
                            res = a
                            
        return res
